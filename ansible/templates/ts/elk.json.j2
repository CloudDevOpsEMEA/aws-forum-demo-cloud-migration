{
    "class": "Telemetry",
    "My_System": {
        "class": "Telemetry_System",
        "allowSelfSignedCert": true,
        "systemPoller": {
            "interval": 60
        }
    },    
    "My_Consumer": {
        "class": "Telemetry_Consumer",
        "type": "ElasticSearch",
        "host": "{{ elk_private_ip }}",
        "index": "bigip",
        "port": 9200,
        "protocol": "http",
        "dataType": "f5.telemetry"
    },
    "My_Listener": {
        "class": "Telemetry_Listener",
        "port": 6514
    }
}
